@font-face {
font-family: "{{ name }}";
src: {{{ fontSrc }}};
}

{{# if selector }}
  {{ selector }} {
{{ else }}
  {{ tag }}[class^="{{prefix}}"]{
{{/ if }}
font-family: {{ name }} !important;
font-style: normal;
font-weight: normal !important;
font-variant: normal;
text-transform: none;
line-height: 1;
-webkit-font-smoothing: antialiased;
-moz-osx-font-smoothing: grayscale;
}
